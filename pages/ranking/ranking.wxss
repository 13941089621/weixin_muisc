.cl_albumtext{
  /* width: fit-content; */
  min-width: 10px;
  background:red;
  white-space:nowrap;
  position: absolute;
  bottom: -9px;
  left: 50%;
  transform: translate(-50%,0);
  font-size: 10px;
  padding: 2px 4px;
  box-sizing: border-box;
  border-radius: 18px;
  color: #fff;
  box-shadow: 1px 2px 2px 1px rgba(0,0,0,0.05);
}

