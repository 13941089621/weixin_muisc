



/* 2 */
.title_top{
  padding: 6px 6px 0 6px;
  box-sizing: border-box;
}
.ov{
  width: 98%;
  padding: 4px 0;
  box-sizing: border-box;
  overflow: hidden;
  margin: 6px auto;
  background:#9d71f3;
  color: #848282;
  border-radius: 4px;
}
.btngo{
  float: left;
  width: 40px;
  height: 36px;
  line-height: 36px;
  text-align: center;
  background: #e4e4e4;
  border-radius: 4px;
  margin-left: 4px;
  color: #999;
  font-size: 12px;
}
.inputs{
  float: left;
  height: 36px;
  width: calc(100% - 60px);
  width: -webkit-calc(100% - 60px);
  margin-left: 8px;
  border-radius: 4px;
  background: #e0dfdf;
  color: #848282;
  padding-left: 6px;
  box-sizing: border-box;
}
.title_top{
  color: #666666;
  font-size: 14px;
}
.title_fl{
  margin: 10px;
  overflow: hidden;
}
.title_name{
  padding: 3px 8px;
  box-sizing: border-box;
  margin-right: 8px;
  background-color: rgba(179, 169, 169,0.2);
  border-radius: 6px;
  font-size: 13px;
  color: #555;
  margin-bottom: 12px;
  float: left;
}
.cl_dh{
  width: 100%;
  height: 30px;
  position: relative;
  background: linear-gradient(to bottom, #833ab4, #833ab4);
  top: 0;
}
.cl_gif{
  position: absolute;
  left: 0;
  bottom: -2rpx;
  width: 100%;
  mix-blend-mode: screen;
  height: 100rpx;
}