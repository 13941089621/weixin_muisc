page{
  background: #fff;
}

.cl_pc_login{
  width: 92%;
  margin: 0 auto;
}
.cl_pc_login_iogo{
  width: 90%;
  height: 200px;
  margin: 30px auto;
  border: 1px solid red;
}
.cl_pc_login_name{
  width: 100%;
  border-bottom: 1px solid rgba(0,0,0,0.02);
  box-shadow: 0px 4px 4px 0px rgba(150, 150, 150, 0.1);
  margin-bottom: 20px;
  overflow: hidden;
}
.cl_pc_login_typetext{
  padding-left: 10px;
  box-sizing: border-box;
}
.placeholder{
  font-size: 10px;
  color: rgb(177, 171, 171);
}
.cl_pc_login_btn{
  width: 100%;
  height: 32px;
  margin: 0 auto;
  background: rgb(31, 144, 24);
  border-radius: 4px;
  line-height: 32px;
  text-align: center;
  color: #fff;
  font-size: 13px;
}
.cl_pc_login_style{
  margin-top: -16px;
  font-size: 10px;
}
.cl_pc_login_color{
  color: rgb(5, 173, 173);
  display: none;
}
.no{
  display: none;
}
.bf{
  display: block;
}
.bfs{
  color: rgb(241, 6, 6);
}
.cl_tab{
  float: left;
}
.cl_pc_login_typetext_s{
  width: calc(100% - 60px);
  float: left;
  padding-left: 10px;
  box-sizing: border-box;
}
.cl_pc_login_yzm{
  width: 50px;
  text-align: center;
  float: right;
  color: rgb(241, 6, 6);
  font-size: 14px;
}